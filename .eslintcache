            </>\n        )\n    }\n}\n\nconst msp = (state) => {\n    return {followedUsers: state.followedUsers}\n}\n\nconst mdp = (dispatch) => {\n    return {fetchUsers: () => dispatch(fetchUsers())}\n}\n\nexport default connect(msp, mdp)(SideBar)","/Users/juliazolotarev/Documents/Flatiron/assignments/Mod-5/goodfriend/goodfriend-frontend/src/components/UserProfile.js",[],{"ruleId":"48","severity":1,"message":"49","line":4,"column":23,"nodeType":"50","messageId":"51","endLine":4,"endColumn":31},{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"48","severity":1,"message":"56","line":7,"column":5,"nodeType":"50","messageId":"51","endLine":7,"endColumn":12},"no-unused-vars","'MenuItem' is defined but never used.","Identifier","unusedVar","no-native-reassign",["57"],"no-negated-in-lhs",["58"],"'Segment' is defined but never used.","no-global-assign","no-unsafe-negation"]